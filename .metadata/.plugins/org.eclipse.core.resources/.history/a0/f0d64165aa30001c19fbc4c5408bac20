package com.xworkz.wrapperclass;

public class Wrapperstarter {

	public static void main(String[] args) {
		
		int a=10;
		System.out.println(Integer.bitCount(a)); //bitcount
		System.out.println(Integer.toBinaryString(a));  //binarystring
		System.out.println(Integer.compare(100, 50)); //compare
	

	}

}
