package com.xworkz.array.pincodes;

public class Pincodestarter {

	public static void main(String[] args) {
		long bidar=585401L;
		long bidars=585402L;
		long bidarn=585403L;
		long blore1=560001L;
		long blore2=560085L;
		
		long[] pincodes= {bidar,bidars,bidarn,blore1,blore2,560070,560059,560029};
		System.out.println(pincodes[0]);
		System.out.println(pincodes[1]);
		System.out.println(pincodes[2]);
		System.out.println(pincodes[3]); //pincodes
		System.out.println(pincodes[4]);
		System.out.println(pincodes[5]);
		System.out.println(pincodes[6]);
		
		
		long orderid=9108222564789L;
		long orderid2=9108222564799L;
		long orderid3=39108222564769L;
		long orderid4=9108222564759L;
		long orderid5=9108222564749L;
		long orderid6=9108222564739L;
		long orderid7=9108222564729L;
		
		long[] orders= {orderid,orderid2,orderid3,orderid4,orderid5,orderid6,orderid7,7894561230321L};
		long index=orders[5];
		System.out.println(index);
		long index2=orders[6];
		System.out.println(index2); //orderid
		long index3=orders[7];
		System.out.println(index3);
		
		
		long markscard1=4050233L;
		long markscard2=4050233L;
		long markscard3=4050233L;
		long markscard4=4050233L;
		long markscard5=4050233L;
		long markscard6=4050233L;
		long markscard7=4050233L;
		long markscard8=4050233L;
		long markscard9=4050233L;
		long markscard10=4050233L;
		
		long[] marksinfo= {markscard1,markscard2,789456123L,markscard3,markscard4,markscard5,markscard6,123415263L,markscard8,markscard7,markscard9,markscard10};
		System.out.println(markscard1);
		for(i=0;i<marksinfo;i++) {
			System.out.println(i);
			System.out.println(marksinfo);
		}
		
		
		
		
	
	
	
	
	
	
	
	
	
	}
	
	
	
	

}
