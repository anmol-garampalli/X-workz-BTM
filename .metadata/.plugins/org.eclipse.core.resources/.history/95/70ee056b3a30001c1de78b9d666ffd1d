package com.xworkz.Associations;

public class Associations {

	public static void main(String[] args) {
		class Bank
		{
		    private String name;
		     
		    // bank name
		    Bank(String name)
		    {
		        this.name = name;
		    }
		     
		    public String getBankName()
		    {
		        return this.name;
		    }
		}
		 
		// employee class
		class Employee
		{
		    private String name;
		     
		    // employee name
		    Employee(String name)
		    {
		        this.name = name;
		    }
		     
		    public String getEmployeeName()
		    {
		        return this.name;
		    }
		}

	}

}
